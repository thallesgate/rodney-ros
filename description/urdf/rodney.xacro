<joint name="chassis_link_joint" type="fixed">
  <parent link="base_link"/>
  <child link="chassis_link"/>
</joint>

<joint name="left_wheel_joint" type="continuous">
  <origin xyz="0.0 0.075834 0.0" rpy="0 0 0"/>
  <parent link="chassis_link"/>
  <child link="left_wheel"/>
  <axis xyz="-0.0 1.0 0.0"/>
</joint>

<joint name="right_wheel_joint" type="continuous">
  <origin xyz="0.0 -0.075166 0.0" rpy="0 0 0"/>
  <parent link="chassis_link"/>
  <child link="right_wheel"/>
  <axis xyz="0.0 -1.0 -0.0"/>
</joint>

<joint name="caster_bearing_joint" type="continuous">
  <origin xyz="-0.106985 0.000334 -0.003964" rpy="0 0 0"/>
  <parent link="chassis_link"/>
  <child link="caster_bearing"/>
  <axis xyz="0.0 0.0 -1.0"/>
</joint>

<joint name="caster_wheel_joint" type="continuous">
  <origin xyz="-0.00971 0.0 -0.023" rpy="0 0 0"/>
  <parent link="caster_bearing"/>
  <child link="caster_wheel"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

